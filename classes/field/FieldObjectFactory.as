package field {	import field.FieldObject;	import field.Santa;	import field.IceBlock;	import field.Block;	import field.IceHole;	import field.LevBlock;	import field.MiscObject;	import field.Door;		public class FieldObjectFactory {		public static function createObject(type:uint, ix:uint, jx:uint):FieldObject {			if (type == FieldObject.SANTA)				return new Santa(ix, jx);			else			if (type == FieldObject.ICE_BLOCK)				return new IceBlock(ix, jx);			else			if (type == FieldObject.BLOCK)				return new Block(ix, jx);			else			if (type == FieldObject.ICE_HOLE)				return new IceHole(ix, jx);			else 			if (type == FieldObject.DOOR)				return new Door(ix, jx);			else			if (type >= FieldObject.LEVBLOCK_LOW && type <= FieldObject.LEVBLOCK_HIGH)				return new LevBlock(type, ix, jx);			else 			if (type == FieldObject.EXIT_TREE || type == FieldObject.LAMP || type == FieldObject.FLOWER_1 || 				type == FieldObject.SNOW_BOX || type == FieldObject.FLOWER_2 || type == FieldObject.SNOWBALL_2 || 				type == FieldObject.SNOWMAN_1 || type == FieldObject.SNOWMAN_2 || type == FieldObject.JUMPING_BALL ||				type == FieldObject.SNOWMAN_3 || type == FieldObject.SQUIRREL || type == FieldObject.TREE_WITH_EYES ||				type == FieldObject.DEER || type == FieldObject.RABBIT || type == FieldObject.SNOWBALL_1)				return new MiscObject(type, ix, jx);			else				return new FieldObject(type, ix, jx, true);		}	}}