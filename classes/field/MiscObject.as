package field {	import flash.display.MovieClip;	import field.FieldObject;		public class MiscObject extends FieldObject {		private var isConstructor:Boolean;				public function MiscObject(type:uint=FieldObject.EXIT_TREE, ix:uint=0, jx:uint=0) {			super(type, ix, jx, false);			isConstructor = true;			if (type == FieldObject.EXIT_TREE)				this.gotoAndPlay("tree_dance");			else if (type == FieldObject.LAMP)				this.gotoAndPlay("lamp");			else if (type == FieldObject.FLOWER_1)				this.gotoAndStop("flower_1");			else if (type == FieldObject.SNOW_BOX)				this.gotoAndStop("snow_box");			else if (type == FieldObject.FLOWER_2)			 	this.gotoAndStop("flower_2");			else if (type == FieldObject.SNOWBALL_2)				this.gotoAndStop("snowball_2");			else if (type == FieldObject.SNOWMAN_1)				this.gotoAndPlay("snowman_1");			else if (type == FieldObject.SNOWMAN_2)				this.gotoAndPlay("snowman_2");			else if (type == FieldObject.JUMPING_BALL)				this.gotoAndPlay("jumping_ball");			else if (type == FieldObject.SNOWMAN_3)				this.gotoAndPlay("snowman_3");			else if (type == FieldObject.SQUIRREL)				this.gotoAndPlay("squirrel");			else if (type == FieldObject.TREE_WITH_EYES)				this.gotoAndPlay("tree_with_eyes");			else if (type == FieldObject.DEER)				this.gotoAndPlay("deer");			else if (type == FieldObject.RABBIT)				this.gotoAndPlay("rabbit");			else if (type == FieldObject.SNOWBALL_1)				this.gotoAndPlay("snowball_1");		}	}}